# editorconfig.org

root = true

[*.js]
indent_style = space
indent_size = 2
end_of_line = LF
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*]
charset                  = utf-8
end_of_line              = LF
trim_trailing_whitespace = true
insert_final_newline     = true